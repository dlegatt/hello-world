{
    "name": "dlegatt/hello-world",
    "description": "A Hello World PHP library",
    "type": "library",
    "version": "1.0.0",
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "David Legatt",
            "email": "dlegatt@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Dlegatt\\HelloWorld\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
